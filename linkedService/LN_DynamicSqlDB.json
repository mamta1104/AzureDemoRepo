{
	"name": "LN_DynamicSqlDB",
	"type": "Microsoft.DataFactory/factories/linkedservices",
	"properties": {
		"parameters": {
			"DBName": {
				"type": "string"
			},
			"UserName": {
				"type": "String"
			},
			"Password": {
				"type": "String"
			}
		},
		"annotations": [],
		"type": "AzureSqlDatabase",
		"typeProperties": {
			"connectionString": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=azuresqlserverdemo03.database.windows.net;Initial Catalog=sampledb;User ID=@{linkedService().UserName};Password=@{linkedService().Password}"
		}
	}
}