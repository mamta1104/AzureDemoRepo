{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactorytutorialazure"
		},
		"LN_BlobDynamic_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LN_BlobDynamic'"
		},
		"LN_SQLDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LN_SQLDB'"
		},
		"ln_blobsource_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ln_blobsource'"
		},
		"trig_storage_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/fe5f5557-5ebc-46ca-8130-a760fdaf3b5e/resourceGroups/resourcegp/providers/Microsoft.Storage/storageAccounts/blobstoragetutorials"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafactorytutorialazure_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LN_BlobDynamic_connectionString": {
						"value": "[parameters('LN_BlobDynamic_connectionString')]"
					},
					"LN_SQLDB_connectionString": {
						"value": "[parameters('LN_SQLDB_connectionString')]"
					},
					"ln_blobsource_connectionString": {
						"value": "[parameters('ln_blobsource_connectionString')]"
					},
					"trig_storage_properties_typeProperties_scope": {
						"value": "[parameters('trig_storage_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}